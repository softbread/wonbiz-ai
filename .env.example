# LLM Provider API Keys
# Choose your preferred LLM provider and add the corresponding API key

# OpenAI OSS - https://platform.openai.com/api-keys
VITE_OPENAI_API_KEY=your_openai_api_key_here

# Grok (xAI) - https://console.x.ai/
VITE_GROK_API_KEY=your_grok_api_key_here

# Gemini (Google) - https://aistudio.google.com/app/apikey
VITE_GEMINI_API_KEY=your_gemini_api_key_here

# AssemblyAI (for transcription) - https://www.assemblyai.com/dashboard/signup
VITE_ASSEMBLYAI_API_KEY=your_assemblyai_api_key_here

# LlamaCloud (LlamaIndex) - https://cloud.llamaindex.ai/
VITE_LLAMA_CLOUD_API_KEY=your_llama_cloud_api_key_here

# Voyage AI (for embeddings) - https://dash.voyageai.com/
# Note: Embeddings are handled by the backend server
VITE_VOYAGE_API_KEY=your_voyage_api_key_here

# Embedding Model
VITE_EMBEDDING_MODEL=voyage-context-3

# Backend API URL (Node.js server with MongoDB)
VITE_API_BASE_URL=http://localhost:3001
